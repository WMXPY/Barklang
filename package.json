{
    "author": "WMXPY",
    "bin": {
        "bkc": "bin/bkc"
    },
    "bugs": {
        "url": "https://github.com/WMXPY/Barklang/issues"
    },
    "dependencies": {},
    "description": ":dog: If there are no dogs in Heaven, then when I die I want to go where they went.",
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/code": "^4.0.4",
        "@types/mocha": "^5.2.3",
        "chai": "^4.1.2",
        "code": "^5.2.0",
        "ghoti-cli": "^4.0.1",
        "mocha": "^5.2.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.10.0",
        "typescript": "^3.0.1"
    },
    "devEngines": {
        "node": ">=5.x",
        "npm": ">=4.x"
    },
    "directories": {
        "doc": "doc"
    },
    "homepage": "https://github.com/WMXPY/Barklang#readme",
    "keywords": [
        "javascript",
        "auto"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "name": "bkc",
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "instrument": true,
        "reporter": [
            "json",
            "text-summary"
        ],
        "report-dir": "coverage",
        "require": [
            "ts-node/register"
        ],
        "sourceMap": true
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WMXPY/Barklang.git"
    },
    "scripts": {
        "build": "tsc",
        "hint": "ghoti post -a node-normal",
        "start": "tsc&&node dist/index.js",
        "test": "mocha"
    },
    "version": "3.1.2"
}
